{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/auth/(.*)",
            "dest": "index.js"
        },
        
        {
            "src": "/books/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/cart/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/transaction/(.*)",
            "dest": "index.js"
        }
        
    ]
}
